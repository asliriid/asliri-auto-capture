{
  "name": "@innovatrics/dot-document-auto-capture",
  "version": "3.3.0",
  "license": "MIT",
  "description": "Web component using video stream from camera to automatically capture image of an ID document, while providing necessary instructions.",
  "keywords": [
    "web components",
    "WebRTC",
    "camera",
    "neural networks",
    "ID document capture"
  ],
  "bugs": {
    "url": "https://github.com/innovatrics/dot-js-kit/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/innovatrics/dot-js-kit"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "dev": "cross-env NODE_ENV=demo webpack serve --hot --progress=profile",
    "build:clear": "rimraf build/client",
    "build:webpack": "cross-env NODE_ENV=production DOCUMENT_COUNTLY_APP_KEY=$DOCUMENT_COUNTLY_APP_KEY webpack",
    "build:webpack-demo": "cross-env NODE_ENV=demo webpack",
    "build:babel": "babel src --out-dir dist --extensions '.ts,.tsx'",
    "build:demo": "run-s build:clear build:webpack-demo",
    "build:types": "tsc --project tsconfig.production.json",
    "build:pack": "yarn build && cd dist && yarn pack --filename dot-document-auto-capture-lib.tgz && mv dot-document-auto-capture-lib.tgz ../",
    "build": "run-s build:clear build:webpack build:types",
    "lint": "eslint --ext .ts,.tsx 'src/**/*.ts' 'src/**/*.tsx' --max-warnings 0",
    "type-check": "tsc",
    "test": "mocha -r ts-node/register -r ../../setupTests.ts --extension ts,tsx src/test/*"
  },
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.8.6",
    "@babel/plugin-proposal-nullish-coalescing-operator": "^7.12.1",
    "@babel/plugin-proposal-optional-chaining": "^7.12.7",
    "@babel/plugin-transform-react-jsx": "^7.9.4",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-react": "^7.8.3",
    "@babel/preset-typescript": "^7.8.3",
    "@types/chai": "^4.3.0",
    "@types/enzyme": "^3.10.10",
    "@types/jsdom": "^16.2.14",
    "@types/mocha": "^9.0.0",
    "@types/preact-custom-element": "^4.0.0",
    "@types/sinon": "^10.0.6",
    "@types/styled-components": "^5.1.7",
    "@typescript-eslint/eslint-plugin": "^4.15.2",
    "@typescript-eslint/parser": "^4.15.2",
    "babel-loader": "^8.0.6",
    "browserify": "17.0.0",
    "canvas": "^2.8.0",
    "chai": "^4.3.4",
    "copy-webpack-plugin": "^8.1.1",
    "cross-env": "^7.0.3",
    "css-loader": "^6.5.1",
    "enzyme": "^3.11.0",
    "enzyme-adapter-preact-pure": "^3.3.0",
    "eslint": "^7.20.0",
    "eslint-config-preact": "^1.1.3",
    "eslint-config-prettier": "^6.10.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-react": "^7.22.0",
    "eslint-plugin-react-hooks": "^2.5.0",
    "file-loader": "^6.2.0",
    "html-webpack-plugin": "^4.5.1",
    "jsdom": "^19.0.0",
    "mocha": "^9.1.3",
    "npm-run-all": "^4.1.5",
    "prettier": "^2.2.1",
    "rimraf": "^3.0.2",
    "sinon": "^12.0.1",
    "style-loader": "^3.3.1",
    "ts-node": "^10.4.0",
    "typescript": "^3.8.3",
    "url-loader": "^4.1.1",
    "webpack": "^5.24.1",
    "webpack-cli": "^4.7.0",
    "webpack-dev-server": "^3.11.2"
  },
  "dependencies": {
    "@types/emscripten": "^1.39.5",
    "polished": "^4.1.0",
    "preact": "^10.5.12",
    "preact-custom-element": "^4.2.1",
    "react-measure": "^2.5.2",
    "styled-components": "^5.2.1",
    "tar": "^6.1.11"
  }
}
